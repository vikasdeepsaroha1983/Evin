				"[concat(variables('factoryId'), '/datasets/ds_country_lookup')]",
				"[concat(variables('factoryId'), '/datasets/ds_dim_date')]",
				"[concat(variables('factoryId'), '/datasets/ds_processed_hospitalization_data_daily')]",
				"[concat(variables('factoryId'), '/datasets/ds_processed_hospitalization_data_weekly')]"
			]
		}

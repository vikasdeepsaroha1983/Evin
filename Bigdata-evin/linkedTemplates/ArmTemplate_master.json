{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Bigdata-evin"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"PostgreSql1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'PostgreSql1'"
		},
		"ls_azure_covid_db_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_azure_covid_db'"
		},
		"ls_covid_reporting_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_covid_reporting'"
		},
		"ls_evin_data_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_evin_data'"
		},
		"ls_evin_source_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_evin_source'"
		},
		"PostgreSql1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "102.210.149.42"
		},
		"PostgreSql1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "live_migration"
		},
		"PostgreSql1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "postgres"
		},
		"ls_azure_covid_db_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "ind-covid-db-server.database.windows.net"
		},
		"ls_azure_covid_db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "covid-db"
		},
		"ls_azure_covid_db_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vikas"
		},
		"ls_covid_reporting_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportdata.dfs.core.windows.net/"
		},
		"ls_evin_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://evindata.dfs.core.windows.net/"
		},
		"ls_evin_source_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "192.168.20.170"
		},
		"ls_evin_source_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "live_migration"
		},
		"ls_evin_source_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "postgres"
		},
		"ls_http_ecdc_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Bigdata-evin_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"ls_azure_covid_db_password": {
						"value": "[parameters('ls_azure_covid_db_password')]"
					},
					"ls_covid_reporting_accountKey": {
						"value": "[parameters('ls_covid_reporting_accountKey')]"
					},
					"ls_evin_data_accountKey": {
						"value": "[parameters('ls_evin_data_accountKey')]"
					},
					"ls_azure_covid_db_properties_typeProperties_server": {
						"value": "[parameters('ls_azure_covid_db_properties_typeProperties_server')]"
					},
					"ls_azure_covid_db_properties_typeProperties_database": {
						"value": "[parameters('ls_azure_covid_db_properties_typeProperties_database')]"
					},
					"ls_azure_covid_db_properties_typeProperties_userName": {
						"value": "[parameters('ls_azure_covid_db_properties_typeProperties_userName')]"
					},
					"ls_covid_reporting_properties_typeProperties_url": {
						"value": "[parameters('ls_covid_reporting_properties_typeProperties_url')]"
					},
					"ls_evin_data_properties_typeProperties_url": {
						"value": "[parameters('ls_evin_data_properties_typeProperties_url')]"
					},
					"ls_http_ecdc_properties_typeProperties_url": {
						"value": "[parameters('ls_http_ecdc_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "Bigdata-evin_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"PostgreSql1_password": {
						"value": "[parameters('PostgreSql1_password')]"
					},
					"ls_evin_source_password": {
						"value": "[parameters('ls_evin_source_password')]"
					},
					"PostgreSql1_properties_typeProperties_server": {
						"value": "[parameters('PostgreSql1_properties_typeProperties_server')]"
					},
					"PostgreSql1_properties_typeProperties_database": {
						"value": "[parameters('PostgreSql1_properties_typeProperties_database')]"
					},
					"PostgreSql1_properties_typeProperties_username": {
						"value": "[parameters('PostgreSql1_properties_typeProperties_username')]"
					},
					"ls_evin_source_properties_typeProperties_server": {
						"value": "[parameters('ls_evin_source_properties_typeProperties_server')]"
					},
					"ls_evin_source_properties_typeProperties_database": {
						"value": "[parameters('ls_evin_source_properties_typeProperties_database')]"
					},
					"ls_evin_source_properties_typeProperties_username": {
						"value": "[parameters('ls_evin_source_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'Bigdata-evin_ArmTemplate_0')]"
			]
		}
	]
}
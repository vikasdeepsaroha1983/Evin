{
	"name": "df_getlatestWatermark",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "ls_evin_data",
						"type": "LinkedServiceReference"
					},
					"name": "rawSource"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "getmaxvalue"
				}
			],
			"scriptLines": [
				"parameters{",
				"     table_name as string,",
				"     watermark_column as string,",
				"     ingestion_date as string",
				"}",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet',",
				"     fileSystem: 'raw',",
				"     wildcardPaths:[($table_name/*)]) ~> rawSource",
				"rawSource aggregate() ~> getmaxvalue"
			]
		}
	}
}